int i;
int sum;

void main() {
    sum = 0;
    for (i = 1; i <= 5; i = i + 1) {
        sum = sum + i;
    }
    write("sum= ", sum);

    // for com expressoes vazias -> loop infinito; interrompe com break
    i = 0;
    for (; i < 3; ) {
        write("i= ", i);
        if (i == 1) continue;
        i = i + 1;
        if (i == 3) break;
    }
}
