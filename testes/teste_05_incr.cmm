int x;
int y;

void main() {
    x = 1;
    y = ++x;         // x=2, y=2
    write("y= ", y);
    y = x++;         // x=3, y=2
    write("y= ", y);
    y = --x;         // x=2, y=2
    write("y= ", y);
    y = x--;         // x=1, y=2
    write("y= ", y);
    x += 5;          // x=6
    write("x= ", x);
}
